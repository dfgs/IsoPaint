<UserControl x:Class="IsoPaint.Views.VoxelView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:IsoPaint.Views"
             mc:Ignorable="d" 
             d:DesignHeight="200" d:DesignWidth="200">
	
	<Grid>
		<Path  Stroke="Gray" Fill="Transparent" Stretch="Fill" Tag="Up" Data="M0,0 M1,1 M0,0.25 l0.5,-0.25 l0.5,0.25 l-0.5,0.25 l-0.5,-0.25"/>
		<Path  Stroke="Gray" Fill="Transparent" Stretch="Fill" Tag="Left" Data="M0,0 M1,1 M0,0.25 l0.5,0.25 l0,0.5 l-0.5,-0.25 l0,-0.5"/>
		<Path  Stroke="Gray" Fill="Transparent" Stretch="Fill" Tag="Right" Data="M0,0 M1,1 M0.5,0.5 l0.5,-0.25 l0,0.5 l-0.5,0.25 l0,-0.5"/>
	</Grid>
</UserControl>
